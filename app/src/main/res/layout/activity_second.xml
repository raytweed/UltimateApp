<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Second_Activity"
    tools:layout_editor_absoluteY="25dp"
    >

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerCrop"
        android:src="@drawable/back"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_marginTop="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="40dp"
        android:text="I.D"
        android:textColor="#fff205"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="16dp"
        android:layout_alignParentLeft="true"/>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:paddingStart="40dp"
        android:layout_height="wrap_content"
        android:text="Name"
        android:textColor="#fff205"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="240dp"
        android:layout_below="@id/s_id"
        />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email"
        android:textColor="#fff205"
        android:textSize="18sp"
        android:paddingStart="40dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="316dp"
        android:layout_below="@id/s_name"/>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Password"
        android:paddingStart="40dp"
        android:textColor="#fff205"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="98dp"
        android:layout_below="@id/s_email"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact"
        android:textColor="#fff205"
        android:textSize="18sp"
        android:textStyle="bold"
        android:paddingStart="40dp"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="394dp"
        android:layout_below="@id/s_pass"/>

    <EditText
        android:id="@+id/s_id"
        android:layout_width="310dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:hint="Enter id"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="44dp"
        android:layout_below="@id/textView3"/>

    <EditText
        android:id="@+id/s_pass"
        android:layout_width="310dp"
        android:layout_height="wrap_content"
        android:hint="Enter password"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="187dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_below="@id/textView6"/>

    <EditText
        android:id="@+id/s_name"
        android:layout_width="310dp"
        android:layout_height="wrap_content"
        android:hint="Enter name"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="267dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_below="@id/textView4"/>

    <EditText
        android:id="@+id/s_email"
        android:layout_width="310dp"
        android:layout_height="wrap_content"
        android:hint="Enter your email"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="344dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_below="@id/textView5"/>

    <EditText
        android:id="@+id/s_contact"
        android:layout_width="310dp"
        android:layout_height="wrap_content"
        android:hint="Enter contact number"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="423dp"
        android:layout_marginLeft="40dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_below="@id/textView7"/>

    <Button
        android:id="@+id/submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Submit"
        tools:layout_editor_absoluteX="135dp"
        android:background="@drawable/create_acnt"
        tools:layout_editor_absoluteY="490dp"
        android:layout_marginLeft="150dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:layout_below="@id/s_contact"/>
</RelativeLayout>